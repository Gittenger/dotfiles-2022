{
	"bracketPairColorizer.depreciation-notice": false,
	"security.workspace.trust.untrustedFiles": "open",
	"telemetry.telemetryLevel": "off",
	"files.autoSave": "off",
	"update.mode": "none",
	"debug.internalConsoleOptions": "neverOpen",
	"code-runner.runInTerminal": true,

	"window.newWindowDimensions": "inherit",
	"window.titleBarStyle": "custom",

	"workbench.activityBar.visible": false,
	"workbench.iconTheme": "icyicons",
	"workbench.colorTheme": "Dracula",
	"workbench.colorCustomizations": {
		"titleBar.activeBackground": "#1d1e22",
		"titleBar.inactiveBackground": "#20232c",
		"titleBar.activeForeground": "#FFB86C",
		"titleBar.inactiveForeground": "#7a6c5c",

		"terminal.background": "#282a36",
		"terminal.foreground": "#f8f8f2",
		"terminalCursor.background": "#6272A4",
		"terminalCursor.foreground": "#f8f8f2",
		"terminal.ansiBlack": "#282936",
		"terminal.ansiBlue": "#BD93F9",
		"terminal.ansiBrightBlack": "#6272a4",
		"terminal.ansiBrightBlue": "#D6ACFF",
		"terminal.ansiBrightCyan": "#A4FFFF",
		"terminal.ansiBrightGreen": "#69FF94",
		"terminal.ansiBrightMagenta": "#FF92DF",
		"terminal.ansiBrightRed": "#ff5555",
		"terminal.ansiBrightWhite": "#F7F7FB",
		"terminal.ansiBrightYellow": "#FFFFA5",
		"terminal.ansiCyan": "#8BE9FD",
		"terminal.ansiGreen": "#50fa7b",
		"terminal.ansiMagenta": "#FF79C6",
		"terminal.ansiRed": "#ff5555",
		"terminal.ansiWhite": "#E9E9F4",
		"terminal.ansiYellow": "#f1fa8c"
	},
	"workbench.editorAssociations": {
		"*.ipynb": "jupyter.notebook.ipynb"
	},
	"debug.javascript.defaultRuntimeExecutable": {
		"pwa-node": "~/.local/share/nvm/versions/node/v16.14.0/bin/node"
	},

	"explorer.compactFolders": false,

	"editor.fontFamily": "JetBrains Mono",
	"editor.fontSize": 18,
	"window.zoomLevel": 1,
	"editor.fontLigatures": false,
	"editor.wordWrap": "on",
	"editor.formatOnSave": true,
	"editor.formatOnPaste": true,
	"editor.minimap.enabled": false,
	"editor.cursorStyle": "line",
	"editor.insertSpaces": false,
	"editor.lineNumbers": "on",
	"editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
	"editor.tabSize": 3,
	"editor.renderWhitespace": "selection",
	"editor.multiCursorModifier": "ctrlCmd",
	"editor.columnSelection": false,

	"files.watcherExclude": {
		"**/.git/objects/**": true,
		"**/.git/subtree-cache/**": true,
		"**/node_modules/**": true
	},
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/node_modules": true,
		"**/package-lock.json": true,
		"**/.gitignore": true,
		"**/*.swp": true
	},

	"terminal.integrated.profiles.linux": {
		"zsh": {
			"path": "/usr/bin/zsh"
		}
	},
	"terminal.integrated.fontSize": 15,

	"[javascriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"editor.quickSuggestions": {
		"comments": "on",
		"strings": "on",
		"other": "on"
	},
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[xml]": {
		"editor.defaultFormatter": "PrateekMahendrakar.prettyxml"
	},
	"[plaintext]": {
		"editor.quickSuggestions": {
			"comments": "off",
			"strings": "off",
			"other": "off"
		}
	},

	"prettier.useTabs": true,
	"prettier.tabWidth": 1,
	"prettier.arrowParens": "avoid",
	"prettier.singleQuote": true,
	"prettier.semi": false,

	"emmet.includeLanguages": {
		"javascript": "javascriptreact",
		"edge": "html"
	},
	"javascript.suggest.paths": false,
	"css.validate": false,
	"tailwindCSS.validate": true,
	"tailwindCSS.emmetCompletions": true,
	"path-autocomplete.extensionOnImport": true,
	"vim.smartRelativeLine": true,
	"vim.vimrc.enable": true,
	"vim.vimrc.path": "~/.vimrc",

	"vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
	"vscode-neovim.neovimInitVimPaths.linux": "~/.config/nvim-vscode/init.vim",
	"python.autoComplete.extraPaths": [
		"~/.local/share/miniconda3/envs/vscode/bin/python3"
	],
	"python.defaultInterpreterPath": "~/.local/share/miniconda3/envs/vscode/bin/python3",
	"python.formatting.provider": "autopep8",
	"python.formatting.autopep8Path": "~/.local/share/miniconda3/envs/vscode/bin/autopep8",
	"python.languageServer": "Jedi",
	"markdown.preview.fontSize": 18,
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[scss]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[css]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"terminal.integrated.inheritEnv": false
}
